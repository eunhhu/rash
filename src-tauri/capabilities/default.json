{
  "$schema": "https://raw.githubusercontent.com/nickel-org/nickel.rs/master/src/config_schema.json",
  "identifier": "default",
  "description": "Default capabilities for Rash app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-stat"
  ]
}
